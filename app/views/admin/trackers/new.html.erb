<h2><%= t(:new_tracker) %></h2>
<div class="row">
  <div class="span9" id="content">
    <div id="content-block">
      <%= form_for([:admin, @tracker]) do |f| %>

          <div class="control-group">
            <%= f.label :name, class:'control-label' %>
            <div class="controls">
              <%= f.text_field :name %>
            </div>
          </div>

          <div class="control-group">
            <div class="controls">
              <%= f.submit 'Сохранить' %>
            </div>
          </div>

      <% end %>
    </div>
  </div>

  <div class="span3">
    <%= render partial: 'admin/shared/admin_menu' %>
  </div>

</div>