<tr>
  <td><%= user_list.name %></td>
  <td><%= user_list.email %></td>
  <td><i class="<%= 'icon-ok' if user_list.is_admin? %>"></i></td>
  <td><%= user_list.created_at.strftime("%d.%m.%Y") %></td>
  <td><%= user_list.last_sign_in_at.strftime("%d.%m.%Y") if user_list.last_sign_in_at.present? %></td>
  <td>
    <%= link_to admin_role_path(user_list), method: :delete, confirm: t(:confirm) do %>
        <i class="icon-trash"></i> <%= t(:delete) %>
    <% end %>
    |
    <%= link_to edit_admin_user_path(user_list) do %>
        <i class="icon-edit"></i> <%= t(:edit) %>
    <% end %>
  </td>
</tr>