<h2>Задачи</h2>
<div class="row">
  <div class="span12">
    <div id="content-block">
      <table class="table table-hover">
        <tr><th>#</th><th>Трекер</th><th>Статус</th><th>Приоритет</th><th>Тема</th><th>Назначена</th><th>Обновлена</th></tr>
        <%= render partial: 'issues/issue_list', collection: @issues %>
      </table>
      <div class="pagination">
        <%= will_paginate @issues, renderer: BootstrapPagination::Rails %>
      </div>
    </div>
  </div>
</div>
